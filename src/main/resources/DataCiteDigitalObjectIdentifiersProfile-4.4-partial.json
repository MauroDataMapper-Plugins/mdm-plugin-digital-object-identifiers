        "minMultiplicity": 1,
        "maxMultiplicity": 1,
        "dataType": "String"
      },
      {
        "fieldName": "Name Type",
        "metadataPropertyName": "creators/creator/creatorNameType",
        "minMultiplicity": 0,
        "maxMultiplicity": 1,
        "dataType": "Enumeration",
        "allowedValues": [
          "Organizational",
          "Personal"
        ]
      },
      {
        "fieldName": "Given Name",
        "metadataPropertyName": "creators/creator/givenName",
        "minMultiplicity": 0,
